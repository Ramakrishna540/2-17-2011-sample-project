node {
    stage("Git Clone") {
        checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Ramakrishna540/2-17-2011-sample-project.git']]])
    }
    stage('maven Build') {
        sh "mvn clean install"
    }
    stage("Docker build"){
        sh 'docker build -t sampleweb .'
        sh 'docker image list'
        sh 'docker tag sampleweb rama80081/sample:latest'
    } 
}
